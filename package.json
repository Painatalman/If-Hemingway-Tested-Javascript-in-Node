{
  "name": "If-Hemingway-tested-Javascript-in-Node",
  "version": "0.3.0",
  "description": "Implementation of Angus Croll's If Hemingway wrote Javascript popular author-based js implementations for solving problems... also, tests",
  "repository": "https://github.com/Painatalman/If-Hemingway-Tested-Javascript-in-Node",
  "main": "index.js",
  "scripts": {
    "test-factorials": "mocha --runner=nyan --compilers coffee:coffee-script/register ./tests/factorial.spec.js",
    "test-primes": "mocha --runner=nyan ./tests/prime.spec.js",
    "test-say-it": "mocha ./tests/sayit.spec.js",
    "test-fibonacci": "mocha --runner=nyan ./tests/fibonacci.spec.js",
    "test-happy": "mocha --runner=nyan ./tests/happy.spec.js",
    "report-say-it ": "istanbul cover node_modules/mocha/bin/_mocha ./tests/sayit.spec.js",
    "UNUSED-test-babel": "mocha --require babel-core/register ./tests/sayit.spec.js"
  },
  "keywords": [
    "books",
    "algorithms",
    "teaching",
    "testing",
    "javascript",
    "coffeescript"
  ],
  "author": "Painatalman, algorithms from Angus Croll",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-preset-es2015": "^6.16.0",
    "chai": "^3.5.0",
    "coffee-script": "^1.11.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.0",
    "mocha-check": "^1.0.0",
    "mocha-testcheck": "^0.1.2",
    "node-notify": "^1.0.0"
  }
}
